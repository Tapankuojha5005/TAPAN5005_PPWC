#include <stdio.h>    

int main()  {  
int n1, n2;   
int sum, sub, mul;
float div; 

printf("Enter any two numbers : ");  
scanf("%d ,%d", &n1 ,&n2); 

sum = n1 + n2;  
sub = n1 - n2;    
mul = n1 * n2;  
div = (float)n1 / n2; 

printf("The sum of n1 and n2 is : %d\n", sum); 
printf("The difference of n2 from n1 is : %d\n", sub); 
printf("The product of n1 and n2 is: %d\n", mul);   
printf("The quotient of n1 and n2 is : %f\n", div);   
  
return 0;   
}
